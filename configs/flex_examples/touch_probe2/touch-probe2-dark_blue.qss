	background-color: black;
	color: white;
	}
	
QMenu::item:disabled {
	background-color: black;
	color: darkgray;
	}

QMenu::icon:checked { /* appearance of a 'checked' icon */
    background: gray;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
	}

QMenu::indicator {
    width: 13px;
    height: 13px;
	}

